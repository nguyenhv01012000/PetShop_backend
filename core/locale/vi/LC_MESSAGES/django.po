#: validators.py:43
msgid "This field must be unique."
msgstr "Trường này không cho phép được trùng với dữ liệu có sẵn."

#: validators.py:97
msgid "The fields {field_names} must make a unique set."
msgstr "Các trường này không cho phép được trùng với dữ liệu có sẵn."

#: validators.py:245
msgid "This field must be unique for the \"{date_field}\" date."
msgstr "Trường này không cho phép được trùng ngày với \"{date_field}\""

#: validators.py:260
msgid "This field must be unique for the \"{date_field}\" month."
msgstr "Trường này không cho phép được trùng tháng với \"{date_field}\""

