#: exceptions.py:49
msgid "A server error occurred."
msgstr "Máy chủ có lỗi xảy ra"

#: exceptions.py:84
msgid "Malformed request."
msgstr "Yêu cầu sai định dạng"

#: exceptions.py:89
msgid "Incorrect authentication credentials."
msgstr "Thông tin đăng nhập bị sai"

#: exceptions.py:94
msgid "Authentication credentials were not provided."
msgstr "Thông tin đăng nhập không được cung cấp"

#: exceptions.py:99
msgid "You do not have permission to perform this action."
msgstr "Bạn không có quyền thực hiện yêu cầu này"

#: exceptions.py:104 views.py:81
msgid "Not found."
msgstr "Không tìm thấy"

#: exceptions.py:109
msgid "Method \"{method}\" not allowed."
msgstr "Hàm \"{method}\" không được phép thực thi."

#: exceptions.py:120
msgid "Could not satisfy the request Accept header."
msgstr "Không thể thỏa mãn header của yêu cầu"

#: exceptions.py:132
msgid "Unsupported media type \"{media_type}\" in request."
msgstr "Yêu cầu có kiểu file \"{media_type}\" không được hỗ trợ."

#: exceptions.py:145
msgid "Request was throttled."
msgstr "Yêu cầu bị hạn chế do vượt quá tốc độ xử lý máy chủ"

#: fields.py:269 relations.py:206 relations.py:239 validators.py:98
#: validators.py:181
msgid "This field is required."
msgstr "Trường này không được bỏ trống"

#: fields.py:270
msgid "This field may not be null."
msgstr "Trường này phải có giá trị."

#: fields.py:608 fields.py:639
msgid "\"{input}\" is not a valid boolean."
msgstr "\"{input}\" không có định dạng đúng hoặc sai."

#: fields.py:674
msgid "This field may not be blank."
msgstr "Trường này không được bỏ trống"

#: fields.py:675 fields.py:1675
msgid "Ensure this field has no more than {max_length} characters."
msgstr "Đảm bảo rằng trường này không có nhiều hơn {max_length} ký tự."

#: fields.py:676
msgid "Ensure this field has at least {min_length} characters."
msgstr "Đảm bảo rằng trường này có ít nhất {min_length} ký tự"

#: fields.py:713
msgid "Enter a valid email address."
msgstr "Hãy nhập đúng định dạng thư điện tử (email)"

#: fields.py:724
msgid "This value does not match the required pattern."
msgstr "Giá trị này không khớp với mẫu cho sẵn."

#: fields.py:735
msgid ""
"Enter a valid \"slug\" consisting of letters, numbers, underscores or "
"hyphens."
msgstr "Hãy nhập \"slug\" hợp lệ chứa chữ cái, số, dấu gạch dưới hoặc "
"dấu phẩy."

#: fields.py:747
msgid "Enter a valid URL."
msgstr "Hãy thêm 1 địa chỉ web hợp lệ"

#: fields.py:760
msgid "\"{value}\" is not a valid UUID."
msgstr "\"{value}\" không phải giá trị UUID hợp lệ."

#: fields.py:796
msgid "Enter a valid IPv4 or IPv6 address."
msgstr "Hãy nhập 1 địa chỉ IPv4 hoặc IPv6 hợp lệ."

#: fields.py:821
msgid "A valid integer is required."
msgstr "Cần nhập 1 số nguyên"

#: fields.py:822 fields.py:857 fields.py:891
msgid "Ensure this value is less than or equal to {max_value}."
msgstr "Đảm bảo rằng giá trị này nhỏ hơn hoặc bằng {max_value}."

#: fields.py:823 fields.py:858 fields.py:892
msgid "Ensure this value is greater than or equal to {min_value}."
msgstr "Đảm bảo rằng giá trị này lớn hơn hoặc bằng {min_value}."

#: fields.py:824 fields.py:859 fields.py:896
msgid "String value too large."
msgstr "Chuỗi ký tự này quá lớn."

#: fields.py:856 fields.py:890
msgid "A valid number is required."
msgstr "Cần nhập 1 chữ số"

#: fields.py:893
msgid "Ensure that there are no more than {max_digits} digits in total."
msgstr "Đảm bảo rằng tổng số ký tự không nhiều hơn {max_digits}."

#: fields.py:894
msgid ""
"Ensure that there are no more than {max_decimal_places} decimal places."
msgstr "Đảm bảo không nhiều hơn {max_decimal_places} chữ số thập phân."

#: fields.py:895
msgid ""
"Ensure that there are no more than {max_whole_digits} digits before the "
"decimal point."
msgstr "Đảm bảo không nhiều hơn {max_whole_digits} chữ số trước "
"dấu phẩy."

#: fields.py:1025
msgid "Datetime has wrong format. Use one of these formats instead: {format}."
msgstr "Giá trị ngày tháng sai định dạng. Dùng 1 trong các định dạng sau: {format}."

#: fields.py:1026
msgid "Expected a datetime but got a date."
msgstr "Yêu cầu thời gian chính xác đến mức giờ."

#: fields.py:1103
msgid "Date has wrong format. Use one of these formats instead: {format}."
msgstr "Ngày bị sai định dạng. Dùng 1 trong các định dạng sau: {format}."

#: fields.py:1104
msgid "Expected a date but got a datetime."
msgstr "Yêu cầu thời gian chính xác đến mức ngày thay vì giờ."

#: fields.py:1170
msgid "Time has wrong format. Use one of these formats instead: {format}."
msgstr "Thời gian sai định dạng. Dùng 1 trong các định dạng sau: {format}."

#: fields.py:1232
msgid "Duration has wrong format. Use one of these formats instead: {format}."
msgstr "Khoảng thời gian sai định dạng. Dùng 1 trong các định dạng sau: {format}."

#: fields.py:1251 fields.py:1300
msgid "\"{input}\" is not a valid choice."
msgstr "\"{input}\" không phải là lựa chọn hợp lệ."

#: fields.py:1254 relations.py:71 relations.py:441
msgid "More than {count} items..."
msgstr "Nhiều hơn {count} mục..."

#: fields.py:1301 fields.py:1448 relations.py:437 serializers.py:524
msgid "Expected a list of items but got type \"{input_type}\"."
msgstr "Tưởng được 1 danh sách các mục (list of items) nhưng lại nhận được \"{input_type}\"."

#: fields.py:1302
msgid "This selection may not be empty."
msgstr "Khoảng chọn này không được để trống."

#: fields.py:1339
msgid "\"{input}\" is not a valid path choice."
msgstr "\"{input}\" không phải là lựa chọn đường dẫn hợp lệ."

#: fields.py:1358
msgid "No file was submitted."
msgstr "Không nhận được tệp nào."

#: fields.py:1359
msgid ""
"The submitted data was not a file. Check the encoding type on the form."
msgstr ""
"Dữ liệu gửi lên không phải là tệp. Kiểm tra kiểu encoding của form."

#: fields.py:1360
msgid "No filename could be determined."
msgstr "Không xác định được tên tệp."

#: fields.py:1361
msgid "The submitted file is empty."
msgstr "Tệp gửi lên là rỗng."

#: fields.py:1362
msgid ""
"Ensure this filename has at most {max_length} characters (it has {length})."
msgstr ""
"Đảm bảo tên tệp dài nhất là {max_length} ký tự (nó dài {length})."
#: fields.py:1410
msgid ""
"Upload a valid image. The file you uploaded was either not an image or a "
"corrupted image."
msgstr ""
"Hãy tải lên 1 ảnh hợp lệ. Tệp tải lên không phải là ảnh hoặc "
" ảnh đã bị hỏng."
#: fields.py:1449 relations.py:438 serializers.py:525
msgid "This list may not be empty."
msgstr "Danh sách này không được để trống."

#: fields.py:1502
msgid "Expected a dictionary of items but got type \"{input_type}\"."
msgstr "Tưởng được 1 từ điển các mục (dictionary of items) nhưng lại được \"{input_type}\"."

#: fields.py:1549
msgid "Value must be valid JSON."
msgstr "Giá trị phải là 1 JSON hợp lệ."

#: filters.py:36 templates/rest_framework/filters/django_filter.html:5
msgid "Submit"
msgstr "Gửi lên"

#: filters.py:336
msgid "ascending"
msgstr "tăng dần"

#: filters.py:337
msgid "descending"
msgstr "giảm dần"

#: pagination.py:193
msgid "Invalid page."
msgstr "Trang không hợp lệ."

#: pagination.py:427
msgid "Invalid cursor"
msgstr "Sai con trỏ."

#: relations.py:207
msgid "Invalid pk \"{pk_value}\" - object does not exist."
msgstr "Không có khóa chính \"{pk_value}\" - đối tượng không tồn tại."

#: relations.py:208
msgid "Incorrect type. Expected pk value, received {data_type}."
msgstr "Sai kiểu. Tưởng nhận được khóa chính, nhận được {data_type}."

#: relations.py:240
msgid "Invalid hyperlink - No URL match."
msgstr "Sai địa chỉ web. Không khớp."

#: relations.py:241
msgid "Invalid hyperlink - Incorrect URL match."
msgstr "Sai địa chỉ web. Không khớp."

#: relations.py:242
msgid "Invalid hyperlink - Object does not exist."
msgstr "Sai địa chỉ web. Đối tượng không tồn tại."

#: relations.py:243
msgid "Incorrect type. Expected URL string, received {data_type}."
msgstr "Sai kiểu. Tưởng nhận được địa chỉ web, nhận được {data_type}."

#: relations.py:401
msgid "Object with {slug_name}={value} does not exist."
msgstr "Đối tượng với {slug_name}={value} không tồn tại."

#: relations.py:402
msgid "Invalid value."
msgstr "Giá trị không hợp lệ."

#: serializers.py:326
msgid "Invalid data. Expected a dictionary, but got {datatype}."
msgstr "Dữ liệu không hợp lệ. Tưởng nhận được từ điển, nhưng nhận được {datatype}."

