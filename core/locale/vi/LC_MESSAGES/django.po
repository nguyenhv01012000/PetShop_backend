#: authentication.py:73
msgid "Invalid basic header. No credentials provided."
msgstr "Sai header cơ bản. Thông tin đăng nhập không được cung cấp"

#: authentication.py:76
msgid "Invalid basic header. Credentials string should not contain spaces."
msgstr "Sai header cơ bản. Chuỗi xác thực không được chứa khoảng trắng"

#: authentication.py:82
msgid "Invalid basic header. Credentials not correctly base64 encoded."
msgstr "Sai header cơ bản. Chuỗi xác thực không được mã hóa base64"

#: authentication.py:99
msgid "Invalid username/password."
msgstr "Sai tên người dùng/mật khẩu"

#: authentication.py:102 authentication.py:198
msgid "User inactive or deleted."
msgstr "Người dùng đã bị khóa hoặc bị xóa"

#: authentication.py:176
msgid "Invalid token header. No credentials provided."
msgstr "Sai token header. Thông tin đăng nhập không được cung cấp"

#: authentication.py:179
msgid "Invalid token header. Token string should not contain spaces."
msgstr "Sai token header. Chuỗi token không được chứa khoảng trắng"

#: authentication.py:185
msgid ""
"Invalid token header. Token string should not contain invalid characters."
msgstr "Sai token header. Chuỗi token không được chứa ký tự không hợp lệ"

#: authentication.py:195
msgid "Invalid token."
msgstr "Sai token"

